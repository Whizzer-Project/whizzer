/************************************************
  Change history:  dd.mon.yyyy  --  author  --   description
  Created:        10.Dec.2020, dd 14522               
  Description:    Returns service name, based on id.
  Parameters:      n/a
  Returns:         service name    
  Used:            FinTP/BASE/DB
***********************************************/

v_servname varchar(100);

BEGIN

  select name into v_servname from fincfg.servicemaps where id = inserviceid;

  return case when v_servname is null then 'n/a' else v_servname end;
     

EXCEPTION
WHEN OTHERS THEN
  return 'n/a';
       
END;
$$;


