--
-- TOC entry 458 (class 1255 OID 2022560)
-- Name: recoverinactiveschemas(character varying); Type: FUNCTION; Schema: fincfg; Owner: fincfg
--

CREATE FUNCTION fincfg.recoverinactiveschemas(incurrenttime character varying) RETURNS void
    LANGUAGE plpgsql
    AS $$
DECLARE

/*
* FinTP - Financial Transactions Processing Application
* Copyright (C) 2013 Business Information Systems (Allevo) S.R.L.
*
* This program is free software: you can redistribute it and/or modify
* it under the terms of the GNU General Public License as published by
* the Free Software Foundation, either version 3 of the License, or
* (at your option) any later version.
*
* This program is distributed in the hope that it will be useful,
* but WITHOUT ANY WARRANTY; without even the implied warranty of
* MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE. See the
* GNU General Public License for more details.
*
* You should have received a copy of the GNU General Public License
* along with this program. If not, see <http://www.gnu.org/licenses/>
* or contact Allevo at : 031281 Bucuresti, 23C Calea Vitan, Romania,
* phone +40212554577, office@allevo.ro <office@allevo.ro>, www.allevo.ro.
*/
/************************************************
  Change history:  dd.mon.yyyy  --  author  --   description
  Created:         27.Aug.2021, dd 15918
  Description:     verifies if hold/release queues are still inactive for inactive schemas
  Parameters:      inCurrentTime - current time [format:HH24:MI:SS]
  Returns:            
  Used:            FinTPc/BASE/RE
***********************************************/

 v_currentTime timestamp;
 v_row			fincfg.routingschemas%rowtype;
 v_row2         fincfg.routingrules%rowtype;
  
BEGIN
 
   v_currentTime := to_timestamp( inCurrentTime,'HH24:MI:SS' );
   
  for v_row in 
   (   select * from fincfg.routingschemas where
      not ( timelimitid1 in ( select id from fincfg.timelimits where ( cutoff - date_trunc( 'day', cutoff ) ) < ( v_currentTime - date_trunc( 'day', v_currentTime ) ) )
       and 
        timelimitid2 in ( select id from fincfg.timelimits where ( cutoff - date_trunc( 'day', cutoff ) ) > ( v_currentTime - date_trunc( 'day', v_currentTime ) ) )
       )
    )
  loop
     for v_row2 in (select * from fincfg.routingrules 
                   where routingschemaid = v_row.id and action in ('ReleaseQueue','HoldQueue'))
     loop
         update fincfg.queues set holdstatus = 1 where id = v_row2.queueid;
     end loop;
  end loop;


EXCEPTION
WHEN OTHERS THEN
   RAISE EXCEPTION 'Unexpected error occured while retrieving active schemas: %', SQLERRM;
       
END;
$$;


ALTER FUNCTION fincfg.recoverinactiveschemas(incurrenttime character varying) OWNER TO fincfg;

